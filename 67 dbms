create database san;
use san;
create table studentnew(rollno int,name varchar(10),marks int,Status varchar(10));
insert into studentnew values(1,"ram",60,"pass"),(2,"sitha",50,"pass"),(3,"diya",40,"null"),(3,"john",30,"null");
select*from studentnew;
DELIMITER @@
create procedure updatestatus1(in mark1 int)
begin
declare mymark int;
set mymark=mark1;
if mymark>35 then
update studentnew set status="pass" where marks =mymark;
else
update studentnew set status="fail" where marks=mymark;
end if;
end;
@@
call updatestatus1(40);
@@
select*from studentnew;

